extends ./layout/board.pug
block content
	.list-wrap
		table.table.table-bordered
			thead
				tr
					th 번호
					th 제목
					th 작성자
					th 작성일
					th 조회수
			tbody
				each list in lists
					tr
						td= list.id
						td
							a(href=`/pug/view?id=${list.id}`)= list.title
						td= list.writer
						td= list.wdate
						td= list.rnum
